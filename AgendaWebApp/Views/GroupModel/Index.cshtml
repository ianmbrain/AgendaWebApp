@model IEnumerable<GroupModel>

@foreach (var item in Model)
{
    <div class="card">
        <div class="card-body">
            <h5 class="card-title">@item.Name</h5>
            <p class="card-text">@item.Description</p>
            <!--<a href="/TodoItemModel/Edit/m@item.Id" class="btn btn-primary">Edit</a>-->
        </div>
    </div>
}
